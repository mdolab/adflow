  <a href="saeddyviscosity_p.html#saeddyviscosity" target="origFile">cv13 = rsacv1**<code class="constant">3</code></a>
<a href="saeddyviscosity_p.html#saeddyviscosity" target="origFile"><code class="comment">! Loop over the cells of this block and compute the eddy viscosity.</code></a>
<a href="saeddyviscosity_p.html#saeddyviscosity" target="origFile"><code class="comment">! Do not include halo's.</code></a>
  <a href="saeddyviscosity_p.html#saeddyviscosity" target="origFile"><code class="keyword">DO </code>k=<code class="constant">2</code>,kl</a>
    <a href="saeddyviscosity_p.html#saeddyviscosity" target="origFile"><code class="keyword">DO </code>j=<code class="constant">2</code>,jl</a>
      <a href="saeddyviscosity_p.html#saeddyviscosity" target="origFile"><code class="keyword">DO </code>i=<code class="constant">2</code>,il</a>
        <a href="saeddyviscosity_p.html#saeddyviscosity" target="origFile">rnusa = w(i, j, k, itu1)*w(i, j, k, irho)</a>
        <a href="saeddyviscosity_p.html#saeddyviscosity" target="origFile">chi = rnusa/rlv(i, j, k)</a>
        <a href="saeddyviscosity_p.html#saeddyviscosity" target="origFile">chi3 = chi**<code class="constant">3</code></a>
        <a href="saeddyviscosity_p.html#saeddyviscosity" target="origFile">fv1 = chi3/(chi3+cv13)</a>
        <a href="saeddyviscosity_p.html#saeddyviscosity" target="origFile">rev(i, j, k) = fv1*rnusa</a>
      <a href="saeddyviscosity_p.html#saeddyviscosity" target="origFile"><code class="keyword">END DO</code></a>
    <a href="saeddyviscosity_p.html#saeddyviscosity" target="origFile"><code class="keyword">END DO</code></a>
