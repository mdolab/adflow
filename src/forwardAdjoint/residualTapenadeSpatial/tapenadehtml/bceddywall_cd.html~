<a href="bceddywall_p.html#bceddywall" target="origFile"><code class="comment">! Determine the face id on which the subface is located and</code></a>
<a href="bceddywall_p.html#bceddywall" target="origFile"><code class="comment">! set the pointers rev1 and rev2 accordingly.</code></a>
  <a href="bceddywall_p.html#bceddywall" target="origFile"><code class="keyword">SELECT CASE </code> (bcfaceid(nn)) </a>
  <a href="bceddywall_p.html#bceddywall" target="origFile"><code class="keyword">CASE </code>(imin) </a>
    <a href="bceddywall_p.html#bceddywall" target="origFile">rev1 => rev(<code class="constant">1</code>, <code class="constant">1</code>:, <code class="constant">1</code>:)</a>
    <a href="bceddywall_p.html#bceddywall" target="origFile">rev2 => rev(<code class="constant">2</code>, <code class="constant">1</code>:, <code class="constant">1</code>:)</a>
  <a href="bceddywall_p.html#bceddywall" target="origFile"><code class="keyword">CASE </code>(imax) </a>
    <a href="bceddywall_p.html#bceddywall" target="origFile">rev1 => rev(ie, <code class="constant">1</code>:, <code class="constant">1</code>:)</a>
    <a href="bceddywall_p.html#bceddywall" target="origFile">rev2 => rev(il, <code class="constant">1</code>:, <code class="constant">1</code>:)</a>
  <a href="bceddywall_p.html#bceddywall" target="origFile"><code class="keyword">CASE </code>(jmin) </a>
    <a href="bceddywall_p.html#bceddywall" target="origFile">rev1 => rev(<code class="constant">1</code>:, <code class="constant">1</code>, <code class="constant">1</code>:)</a>
    <a href="bceddywall_p.html#bceddywall" target="origFile">rev2 => rev(<code class="constant">1</code>:, <code class="constant">2</code>, <code class="constant">1</code>:)</a>
  <a href="bceddywall_p.html#bceddywall" target="origFile"><code class="keyword">CASE </code>(jmax) </a>
    <a href="bceddywall_p.html#bceddywall" target="origFile">rev1 => rev(<code class="constant">1</code>:, je, <code class="constant">1</code>:)</a>
    <a href="bceddywall_p.html#bceddywall" target="origFile">rev2 => rev(<code class="constant">1</code>:, jl, <code class="constant">1</code>:)</a>
  <a href="bceddywall_p.html#bceddywall" target="origFile"><code class="keyword">CASE </code>(kmin) </a>
    <a href="bceddywall_p.html#bceddywall" target="origFile">rev1 => rev(<code class="constant">1</code>:, <code class="constant">1</code>:, <code class="constant">1</code>)</a>
    <a href="bceddywall_p.html#bceddywall" target="origFile">rev2 => rev(<code class="constant">1</code>:, <code class="constant">1</code>:, <code class="constant">2</code>)</a>
  <a href="bceddywall_p.html#bceddywall" target="origFile"><code class="keyword">CASE </code>(kmax) </a>
    <a href="bceddywall_p.html#bceddywall" target="origFile">rev1 => rev(<code class="constant">1</code>:, <code class="constant">1</code>:, ke)</a>
    <a href="bceddywall_p.html#bceddywall" target="origFile">rev2 => rev(<code class="constant">1</code>:, <code class="constant">1</code>:, kl)</a>
  <a href="bceddywall_p.html#bceddywall" target="origFile"><code class="keyword">END SELECT</code></a>
<a href="bceddywall_p.html#bceddywall" target="origFile"><code class="comment">! Loop over the faces of the subface and set the eddy</code></a>
<a href="bceddywall_p.html#bceddywall" target="origFile"><code class="comment">! viscosity in the halo cells.</code></a>
  <a href="bceddywall_p.html#bceddywall" target="origFile"><code class="keyword">DO </code>j=bcdata(nn)%jcbeg,bcdata(nn)%jcend</a>
    <a href="bceddywall_p.html#bceddywall" target="origFile"><code class="keyword">DO </code>i=bcdata(nn)%icbeg,bcdata(nn)%icend</a>
      <a href="bceddywall_p.html#bceddywall" target="origFile">rev1(i, j) = -rev2(i, j)</a>
    <a href="bceddywall_p.html#bceddywall" target="origFile"><code class="keyword">END DO</code></a>
  <a href="bceddywall_p.html#bceddywall" target="origFile"><code class="keyword">END DO</code></a>
<a href="bceddywall_p.html#bceddywall" target="origFile"><code class="keyword">END SUBROUTINE </code><code class="funcname">BCEDDYWALL_CD</code></a>
